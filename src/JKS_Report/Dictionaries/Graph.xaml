                <ControlTemplate TargetType="{x:Type chartingToolkit:Chart}">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0" Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="65px"/>
                            </Grid.RowDefinitions>
                            <visualizationToolkit:Title Content="{TemplateBinding Title}" Style="{TemplateBinding TitleStyle}"/>
                            <Grid Margin="0,15" Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Primitives:EdgePanel x:Name="ChartArea" Style="{TemplateBinding ChartAreaStyle}">
                                    <Grid Style="{TemplateBinding PlotAreaStyle}" Panel.ZIndex="-1"/>
                                    <Border BorderBrush="#FF919191" BorderThickness="0" Panel.ZIndex="10"/>
                                </Primitives:EdgePanel>
                            </Grid>
                            <visualizationToolkit:Legend Grid.Row="2" x:Name="Legend" Grid.Column="1" Header="{TemplateBinding LegendTitle}" Style="{TemplateBinding LegendStyle}" Visibility="{Binding ContentVisibility}"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="GraphStackedBarSeries" TargetType="{x:Type chartingToolkit:StackedBarSeries}">
        <Grid x:Name="SeriesArea" Background="{TemplateBinding Background}">
            <Primitives:DelegatingListBox x:Name="ItemContainer" BorderThickness="0" Background="Transparent">
                <Primitives:DelegatingListBox.ItemsPanel>
                    <ItemsPanelTemplate>
                        <Canvas IsItemsHost="True"/>
                    </ItemsPanelTemplate>
                </Primitives:DelegatingListBox.ItemsPanel>
                <Primitives:DelegatingListBox.ItemContainerStyle>
                    <Style TargetType="{x:Type ListBoxItem}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type ListBoxItem}">
                                    <ContentPresenter Content="{TemplateBinding Content}"/>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Primitives:DelegatingListBox.ItemContainerStyle>
            </Primitives:DelegatingListBox>
        </Grid>
    </ControlTemplate>
    
</ResourceDictionary>