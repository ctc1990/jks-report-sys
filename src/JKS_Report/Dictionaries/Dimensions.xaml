<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CLEANXCEL2._2.Dictionaries"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Double x:Key="Dim100">100</sys:Double>
    <sys:Double x:Key="DimSubMenu">150</sys:Double>
    <sys:Double x:Key="DimSubMenuRoles">200</sys:Double>
    <sys:Double x:Key="ContainerWidth">400</sys:Double>

    <Style x:Key="DimGridSubMenu" TargetType="{x:Type ColumnDefinition}">
        <Setter Property="Width" Value="250"/>
    </Style>

    <Thickness x:Key="PrimarySpacing" Left="0" Right="0"
               Top="30" Bottom="0"/>
    <Thickness x:Key="SecondarySpacing" Left="0" Right="0"
               Top="15" Bottom="0"/>
    <Thickness x:Key="ButtonSpacing" Left="90" Right="0"
               Top="45" Bottom="0"/>


    <Thickness x:Key="FiboSeq1" Left="5" Right="5"
               Top="5" Bottom="5"/>
    <Thickness x:Key="FiboSeq2" Left="13" Right="13"
               Top="13" Bottom="13"/>
    <Thickness x:Key="FiboSeq3" Left="21" Right="21"
               Top="21" Bottom="21"/>
    <Thickness x:Key="FiboSeq4" Left="34" Right="34"
               Top="34" Bottom="34"/>


</ResourceDictionary>